- (UIScrollView *)RollView{
    if (!_RollView) {
        _RollView = [[UIScrollView alloc] initWithFrame:CGRectMake(0, 0, SCREEN_WIDTH, ROLL_HEIGHT)];
        [self addRollBackImg];
        [self.view addSubview:_RollView];
        _RollView.contentSize = CGSizeMake(7*SCREEN_WIDTH, ROLL_HEIGHT);
        
        
    }
    return _RollView;
